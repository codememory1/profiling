{
  "big": {
    "configName": "big",
    "defaultPathWithTemplates": "\/",
    "useCache": false,
    "allowPhpTags": false,
    "debugger": {
      "lines": 20,
      "template": "vendor\/codememory\/big\/Debugger\/templates\/white"
    }
  },
  "caching": {
    "fileConfig": "configs\/caching"
  },
  "version": 1.7,
  "configuration": {
    "pathWithConfigs": "configs",
    "useCache": true
  },
  "environment": {
    "pathWithFile": null,
    "filename": ".env",
    "defaultAppMode": "dev",
    "useCache": false
  },
  "http": {
    "configName": "http"
  },
  "controlHttpStatus": {
    "configName": "httpStatus"
  },
  "routing": {
    "configName": "routing",
    "settings": {
      "pathWithRoutes": "App\/Routing\/Routes",
      "softwareNamespace": "App\\Routing\\Software",
      "routesFileSuffix": "-routes"
    }
  },
  "serviceProvider": {
    "configName": "services"
  },
  "view": {
    "configName": "view",
    "pathWithTemplates": "\/",
    "engine": "Codememory\\Components\\View\\Engines\\BigEngine"
  },
  "profiling": {
    "configName": "profiler",
    "subdomain": "profiler",
    "enabledInProduction": false
  }
}